<?php



function fibonacci($n) {

  if ($n <= 1) {

    return $n;

  } else {

    return fibonacci($n - 1) + fibonacci($n - 2);

  }

}



$sequence_length = 34;

$sum = 0;



for ($i = 0; $i < $sequence_length; $i++) {

  $fibonacci_number = fibonacci($i);

  $sum += $fibonacci_number;

}



echo "$sum";



?>

